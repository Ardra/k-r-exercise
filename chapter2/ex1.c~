#include<stdio.h>
#include<limits.h>
#include<float.h>
main()
{
	printf("Range of different data types\n");
	printf("\n Range  of unsigned char 0----%u \n",UCHAR_MAX);
	printf("\n Range  of unsigned short 0---- %u \n",USHRT_MAX);
	printf("\n Range  of unsigned int 0---- %u \n",UINT_MAX);
	printf("\n Range  of unsigned long 0----%lu \n",ULONG_MAX);

	
	printf("\n Range  of signed char %d----%d \n",SCHAR_MIN,SCHAR_MAX);
	printf("\n Range  of signed short %d----%d \n",SHRT_MIN,SHRT_MAX);
	printf("\n Range  of signed int %ld----%ld \n",INT_MIN,INT_MAX);
	printf("\n Range  of signed long %ld----%ld \n",LONG_MIN,LONG_MAX);//long_min format for printf?

	printf("\n Range of floating point number %f----%f\n",FLT_MIN,FLT_MAX);
	printf("\n Range of double %f----%f\n",DBL_MIN,DBL_MAX);
	




}

